@model string;
@{
    ViewData["Title"] = "Find a regulated awarding organisation";
}
<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds-from-desktop">

        <h1 class="govuk-heading-m">Search results – '@Model'</h1>



        <div class="app-search govuk-!-margin-bottom-4">
            <form action="search" method="get" novalidate="">


                <div class="govuk-form-group">
                    <label class="govuk-label app-search__label govuk-label--s" for="keywords">
                        Search for an organisation
                    </label>

                    <input class="govuk-input app-search__input" id="selected-organisation" name="selected-organisation" type="search" value="@Model">

                </div>

                <button class="govuk-button app-search__button" data-module="govuk-button">
                    Search
                </button>
            </form>
        </div>








        <a href="/download/download-org-results" role="button" draggable="false" class="govuk-button govuk-button--secondary" data-module="govuk-button">
            Download results as CSV
        </a>


        <ul class="govuk-list govuk-list--spaced">
            <li class="results-list-item">
                <p class="govuk-body-!-margin-top-2">
                    <a class="govuk-link govuk-link--no-underline govuk-body govuk-!-font-weight-bold" href="organisation-details">Pearson EDI</a><br>
                    <span class="govuk-body-s">Pearson EDI</span>
                </p>
                <div class="govuk-grid-row">
                    <div class="govuk-grid-column-one-quarter">Ofqual status: </div>
                    <div class="govuk-grid-column-one-half">
                        <strong class="govuk-tag govuk-tag--purple">
                            Surrendered
                        </strong>
                    </div>
                </div>
                <div class="govuk-grid-row">
                    <div class="govuk-grid-column-one-quarter">CCEA status: </div>
                    <div class="govuk-grid-column-one-half">
                        <strong class="govuk-tag govuk-tag--grey">
                            Never recognised
                        </strong>
                    </div>
                </div>
                <p></p>
            </li>
            <li class="results-list-item">
                <p class="govuk-body-!-margin-top-2">
                    <a class="govuk-link govuk-link--no-underline govuk-body govuk-!-font-weight-bold" href="organisation-details">Pearson Education Ltd</a><br>
                    <span class="govuk-body-s">Pearson</span>
                </p>
                <div class="govuk-grid-row">
                    <div class="govuk-grid-column-one-quarter">Ofqual status: </div>
                    <div class="govuk-grid-column-one-half">
                        <strong class="govuk-tag govuk-tag--green">
                            Recognised
                        </strong>
                    </div>
                </div>
                <div class="govuk-grid-row">
                    <div class="govuk-grid-column-one-quarter">CCEA status: </div>
                    <div class="govuk-grid-column-one-half">
                        <strong class="govuk-tag govuk-tag--green">
                            Recognised
                        </strong>
                    </div>
                </div>
            </li>
            <li class="results-list-item">
                <p class="govuk-body-!-margin-top-2">
                    <a class="govuk-link govuk-link--no-underline govuk-body govuk-!-font-weight-bold" href="organisation-details">Person Centred Awards</a><br>
                    <span class="govuk-body-s">PCA</span>
                </p>
                <div class="govuk-grid-row">
                    <div class="govuk-grid-column-one-quarter">Ofqual status: </div>
